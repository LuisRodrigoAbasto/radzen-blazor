      "args": [],
      "cwd": "${workspaceFolder}/RadzenBlazorDemos",
      "stopAtEntry": false,
      "serverReadyAction": {
        "action": "openExternally",
        "pattern": "\\bNow listening on:\\s+(https?://\\S+)"
